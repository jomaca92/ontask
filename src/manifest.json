{
  "manifest_version": 2,
  "name": "__MSG_extName__",
  "version": "0.0.1",
  "homepage_url": "https://dontdistrakt.me/",
  "description": "A Vue Browser Extension",
  "default_locale": "en",
  "permissions": [
    "<all_urls>",
    "*://*/*"
  ],
  "icons": {
    "16": "icons/16.png",
    "32": "icons/32.png",
    "180": "icons/180.png"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_title": "__MSG_extName__",
    "default_icon": {
      "16": "icons/16.png",
      "32": "icons/32.png"
    }
  },
  "chrome_url_overrides" : {
    "newtab": "index.html"
  }
}
